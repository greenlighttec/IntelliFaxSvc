<!DOCTYPE html>
<html lang="en">


<%- include ("./header.ejs",{path: obj.path}) %>


  <!--Main layout Begin-->
  <main style="margin-top: 58px">
    <div class="container pt-4" style="max-width: -webkit-fill-available">

      <section class="mb-4">
        <div class="card">
          <div class="card-header text-center py-3">
            <h5 class="mb-0 text-center">
              <strong><%= obj.cardHeaderName %> View</strong>
            </h5>
          </div>
          <div class="card-body">
            <div class="ag-theme-alpine">
		<div class="<%= obj.tableClassName %>Table" style="height: 300px;"></div>
            </div>
          </div>
        </div>
      </section>
	  <!-- Insert form here -->
		<% if (obj.path == 'customers') { %> 
		<%- include ("./formHeader.ejs") %> <%- include ("./formCustomers.ejs") %> <%- include ("./formFooter.ejs") %>
		<% }
                else if (obj.path == 'users') { %> 
		 <%- include ("./formHeader.ejs") %> <%- include ("./formUsers.ejs") %> <%- include ("./formFooter.ejs") %>
		<% }
		else if (obj.path == 'devices') { %> 
		<%- include ("./formHeader.ejs") %> <%- include ("./formDevices.ejs") %> <%- include ("./formFooter.ejs") %>
		<% } %>

  </main>
  <!--Main layout End-->
  <!-- MDB -->
  <script type="text/javascript" src="/admin/resources/js/mdb.min.js"></script>
  <!-- Custom scripts
  <script type="text/javascript" src="/admin/resources/js/admin.js"></script> -->

 <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
 <script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.3/dist/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
 <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
  <script type="text/javascript" src="/admin/resources/js/<%= obj.frontEndScriptName %>"></script>
  <!-- Latest compiled and minified JavaScript -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.14/dist/js/bootstrap-select.min.js"></script>
  <script src="https://unpkg.com/ag-grid-community/dist/ag-grid-community.min.noStyle.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.min.js" integrity="sha512-d9xgZrVZpmmQlfonhQUvTR7lMPtO7NkZMkA0ABN3PHCbKA5nqylQ/yWlFAyY6hYgdF1Qh6nYiuADWwKB4C2WSw=="
    crossorigin="anonymous"></script>



</body>

</html>
